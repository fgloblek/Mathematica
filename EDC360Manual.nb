x["g",                       TagBox[\((1, 0, 0)\),                        Derivative],                      MultilineFunction->None], "[",                     \(\(x + y\)\/\(x\^2 + y\^2\),                     \(1 + x\ y\)\/\(x\^2 + y\^2\), z\), "]"}]}], "+",                 RowBox[{"2", " ", "x", " ", "y", " ",                   RowBox[{                    SuperscriptBox["g",                       TagBox[\((1, 0, 0)\),                        Derivative],                      MultilineFunction->None], "[",                     \(\(x + y\)\/\(x\^2 + y\^2\),                     \(1 + x\ y\)\/\(x\^2 + y\^2\), z\), "]"}]}], "-",                 RowBox[{\(y\^2\), " ",                   RowBox[{                    SuperscriptBox["g",                       TagBox[\((1, 0, 0)\),                        Derivative],                      MultilineFunction->None], "[",                     \(\(x + y\)\/\(x\^2 + y\^2\),                     \(1 + x\ y\)\/\(x\^2 + y\^2\), z\), "]"}]}]}], ")"}]}],           ")"}]}], "-",       RowBox[{\(1\/\((x\^2 + y\^2)\)\^2\),         RowBox[{"(",           RowBox[{\(d[y]\), " ",             RowBox[{"(",               RowBox[{                RowBox[{\(-x\^3\), " ",                   RowBox[{                    SuperscriptBox["g",                       TagBox[\((0, 1, 0)\),                        Derivative],                      MultilineFunction->None], "[",                     \(\(x + y\)\/\(x\^2 + y\^2\),                     \(1 + x\ y\)\/\(x\^2 + y\^2\), z\), "]"}]}], "+",                 RowBox[{"2", " ", "y", " ",                   RowBox[{                    SuperscriptBox["g",                       TagBox[\((0, 1, 0)\),                        Derivative],                      MultilineFunction->None], "[",                     \(\(x + y\)\/\(x\^2 + y\^2\),                     \(1 + x\ y\)\/\(x\^2 + y\^2\), z\), "]"}]}], "+",                 RowBox[{"x", " ", \(y\^2\), " ",                   RowBox